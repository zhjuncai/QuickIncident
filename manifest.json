{
  "manifest_version": 2,

  "name": "QuickIncident",
  "description": "Help you quickly find an incident in SAP BCP",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "https://support.wdf.sap.corp/sap/support/message/*"
  ]
}